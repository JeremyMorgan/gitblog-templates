doctype html
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic|Anonymous+Pro:400,700,400italic,700italic|Merriweather:400,700,300')
      link(rel='stylesheet', href='//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css')
  body(class=bodyclass)


.navbar.navbar-inverse.navbar-top(role='navigation')
    .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') Hacker Ramblings
        .navbar-collapse.collapse
          ul.nav.navbar-nav
            li.active
              a(href='#') Home
            li
              a(href='#about') About
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                | My Projects
                span.caret
              ul.dropdown-menu(role='menu')
                li
                  a(href='https://github.com/JeremyMorgan/Rowena-Fire-Realtime') Rowena Fire Realtime
                li
                  a(href='https://github.com/JeremyMorgan/bryant-fire-realtime') Bryant Fire Realtime
                li
                  a(href='https://github.com/JeremyMorgan/two-bulls-realtime') Two Bulls Realtime
                li
                  a(href='https://github.com/JeremyMorgan/ASP-Self-Hosted-API') ASP Self Hosted API
                li
                  a(href='https://github.com/JeremyMorgan/Simple-ASP-WebAPI-Example') Simple ASP Web API Example
                li
                  a(href='https://github.com/JeremyMorgan/csharp-coding-guidelines') C# Coding Guidelines
                li
                  a(href='https://github.com/JeremyMorgan/multicontactform') Multi Contact Form
                li
                  a(href='https://github.com/JeremyMorgan/AdSenseCalculator') Adsense Calculator
                li
                  a(href='https://github.com/JeremyMorgan/ipcountrycode') IP to Country Code Converter
                li
                  a(href='https://github.com/JeremyMorgan/firecom') FireCom
                li
                  a(href='https://github.com/JeremyMorgan/ArchScripts') Arch Scripts
                li
                  a(href='https://github.com/JeremyMorgan/golang-API-Test') GoLang API Test
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                | Contact Me
                span.caret
              ul.dropdown-menu(role='menu')
                li
                  a(href='http://www.jeremymorgan.com') My Programming Blog
                li
                  a(href='http://www.jeremymorgan.com/contact/') Contact Jeremy Morgan
                li
                  a(href='http://www.twitter.com/jeremycmorgan') Twitter
                li
                  a(href='https://plus.google.com/+JeremyMorgan?rel=author') Google Plus
                li
                  a(href='http://facebook.com/JeremyMorganDotCom') Facebook
                li
                  a(href='http://geekswithblogs.net/JeremyMorgan/Default.aspx') Geeks with Blogs
                li
                  a(href='https://coderbits.com/coder/JeremyMorgan') CoderBits
                li
                  a(href='http://coderwall.com/jeremymorgan') My CoderWall
    header.header
  container
    .row.clearfix
        .col-md-12.column
          .row.clearfix
            .col-md-1.column
            .col-md-10.column
              block content
            .col-md-1.column

        block footer
          section.about
            !=contents['about.md'].html
          section.copy
            p &copy; #{ new Date().getFullYear() } #{ locals.owner } &mdash; powered by&nbsp;
              a(href='https://github.com/jnordberg/wintersmith') Wintersmith
              //- please leave the "powered by" if you use the design
script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
script(src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js")